  "scripts": {
    "bootstrap": "npm install --no-audit --no-fund",
    "bootstrap:prod": "npm run bootstrap",
    "start:web": "ts-node ./var/web.entry.js --servebuild no --info ../packages/sotaoi-omni/app-info.json",
