    "stop:web:prod": "forever stop ./var/web.entry.js",
    "deploy:web": "node ./scripts/deploy/deploy-web",
    "build:web": "node ./scripts/build/build-web",
    "clean:bootstrap": "node ./scripts/clean/clean-bootstrap",
    "clean:web": "node ./scripts/clean/clean-web",
    "postinstall": "node ./scripts/hook/hook-postinstall"
  },
  "devDependencies": {
